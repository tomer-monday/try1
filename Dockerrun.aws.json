{
   "AWSEBDockerrunVersion": 2,
   "contanerDefinitions": [
       {
           "name": "client",
           "image": "tomerdocker1/multi-client",
           "hostname": "client",
           "essential": false,
           "memory": 128
       },
       {
            "name": "server",
            "image": "tomerdocker1/multi-server",
            "hostname": "api",
            "essential": false,
            "memory": 128

        },
        {
            "name": "worker",
            "image": "tomerdocker1/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128

        },
        {
            "name": "nginx",
            "image": "tomerdocker1/nginx",
            "essential":true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }

   ]
}